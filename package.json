{
  "author": "Muhammad Ubaid Raza <mubaidr@gmail.com>",
  "bugs": {
    "url": "https://github.com/mubaidr/bulma-fluent/issues"
  },
  "description": "Fluent Design Theme for Bulma CSS framework, inspired by Microsoftâ€™s Fluent Design System",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "postcss-cli": "^5.0.0",
    "rimraf": "^2.6.2"
  },
  "files": [
    "css",
    "sass",
    "bulma.sass",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://mubaidr.github.io/bulma-fluent",
  "keywords": [
    "css",
    "bulma",
    "theme",
    "fluent"
  ],
  "license": "MIT",
  "main": "bulma.sass",
  "name": "bulma-fluent",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mubaidr/bulma-fluent.git"
  },
  "scripts": {
    "build": "npm run build-clean && npm run build-sass && npm run build-autoprefix",
    "build-autoprefix": "postcss --use autoprefixer --map false --output css/bulma.css css/bulma.css",
    "build-clean": "rimraf css",
    "build-docs": "cd docs-src && npm run build && cd..",
    "build-sass": "node-sass --output-style expanded --source-map true bulma.sass css/bulma.css",
    "dev": "cd docs-src && npm run dev && cd..",
    "start": "npm run build-sass -- --watch",
    "start-test": "npm run test-sass -- --watch"
  },
  "style": "bulma/css/bulma.css",
  "version": "0.1.7"
}
